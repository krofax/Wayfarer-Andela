    {
        "env": {
            "es6": true,
            "browser": true
        },
        "extends": [
            "airbnb",
            "prettier"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": [
                "error"
            ],
            "no-console": 0,
            "no-unused-vars": [
                1,
                {
                    "argsIgnorePattern": "res|next|^err"
                }
            ],
            "comma-dangle": 0,
            "import": 0,
            "max-len": 0,
            "space-before-function-paren": 0
        }
    }